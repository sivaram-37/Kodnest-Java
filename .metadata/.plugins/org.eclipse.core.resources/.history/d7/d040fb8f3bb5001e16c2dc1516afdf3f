package com.recursion;

public class FibonacciSeries {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=5;
		System.out.print(fab(n));

	}

	private static String fab(int n) {
		
		int a=0,b=1,i=2;
		String s=a+" "+b;
		while(i<=n) {
			int c=a+b;
			a=b;
			b=c;
			s+=c+" ";
		}
		return s;
		
	}

}
